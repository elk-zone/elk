    "@tiptap/starter-kit": "2.0.2",
    "@tiptap/suggestion": "2.0.2",
    "@tiptap/vue-3": "2.0.2",
    "@unocss/nuxt": "^0.51.0",
    "@vue-macros/nuxt": "^1.2.3",
    "@vueuse/core": "^9.13.0",
    "@vueuse/gesture": "2.0.0-beta.1",
    "@vueuse/integrations": "^9.13.0",
