  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@antfu/ni": "^0.21.2",
    "@types/chroma-js": "^2.4.0",
    "@types/file-saver": "^2.0.5",
    "@types/flat": "^5.0.2",
    "@types/fnando__sparkline": "^0.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/prettier": "^2.7.2",
    "@types/wicg-file-system-access": "^2020.9.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "esno": "^0.16.3",
    "flat": "^5.0.2",
    "fs-extra": "^11.1.1",
    "lint-staged": "^13.2.0",
    "nuxt": "3.3.2",
    "prettier": "^2.8.7",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "vue": "3.2.45"
    },
    "patchedDependencies": {
      "nuxt-security@0.13.0": "patches/nuxt-security@0.13.0.patch"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
